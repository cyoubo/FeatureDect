<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.keypointdect"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="19" />

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS" />
    <uses-permission android:name="android.permission.INTERNET" />
    
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity android:name="com.featuredect.activity.ATouchToPickPoint" android:theme="@android:style/Theme.Light.NoTitleBar"></activity>
        <activity android:name="com.featuredect.activity.APhotoPicking" android:theme="@android:style/Theme.Light.NoTitleBar"></activity>
        <activity android:name="com.featuredect.activity.AWaitLoading">
             <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.featuredect.activity.AROIDectorListDisplay" android:theme="@android:style/Theme.Light.NoTitleBar"></activity>
        <activity android:name="com.featuredect.activity.ADectParamSet" android:theme="@android:style/Theme.Holo.Light.Dialog"></activity>
        <activity android:name="com.featuredect.activity.AROIDectResultDisplay" android:theme="@android:style/Theme.Light.NoTitleBar"></activity>
        <activity android:name="com.featuredect.activity.AImagePointListDisplay" android:theme="@android:style/Theme.Light.NoTitleBar"></activity>
        <activity android:name="com.featuredect.activity.ABinaryTest" android:theme="@android:style/Theme.Holo.NoActionBar"></activity>
        <activity android:name="com.featuredect.activity.AFunctionSelected" android:theme="@android:style/Theme.Light.NoTitleBar"></activity>
        <activity android:name="com.featuredect.activity.AImageControlInput" android:screenOrientation="landscape" android:theme="@android:style/Theme.Light.NoTitleBar"></activity>
        <activity android:name="com.featuredect.activity.AExternalSolve" android:theme="@android:style/Theme.Holo.Light.NoActionBar"></activity>
    </application>

</manifest>
